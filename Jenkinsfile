    agent any

    stages {
        stage('Compile') {
            steps {
                echo 'Compiling the code'
            }
        }
        stage('Unitest') {
            steps {
                echo 'Testing the code'
            }
        }
        stage('Package') {
            steps {
                echo 'Packing the code'
            }
        }
        
    }
    
}
